<!DOCTYPE html>
<html>
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>Service Request Lookup</h1>

       <form action>
            <label for="srID">Service Request ID:</label>
            <input type="number" id="srID" name="srID"><br><br>

            <label for="srAddress">Work Site Address:</label>
            <input type="text" id="srAddress" name="srAddress"><br><br>

            <label for="srType"> Service Request:</label>
            <select id="srType">
                <option label="Cleaning Ticket">Cleaning Ticket</option>
                <option label="Waste Ticket">Waste Ticket</option>
              </select><br><br>
            
            <input type="submit" value="Submit">
        </form>
    </div>
</main>


<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>